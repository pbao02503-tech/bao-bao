// 8. Tổng tiền mua hàng
#include <iostream>
using namespace std;
int main() {
    double i1 = 15.59, i2 = 24.59, i3 = 6.59, i4 = 12.59, i5 = 3.59;
    double subtotal = i1 + i2 + i3 + i4 + i5;
    double taxRate = 0.07;
    double tax = subtotal * taxRate;
    double total = subtotal + tax;

    cout << "Subtotal: " << subtotal << endl;
    cout << "Tax: " << tax << endl;
    cout << "Total: " << total << endl;
    return 0;
}
